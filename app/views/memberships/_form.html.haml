= form_for @membership do |f|
  - if @membership.errors.any?
    #error_explanation
      %h2= "#{pluralize(@membership.errors.count, "error")} prohibited this membership from being saved:"
      %ul
        - @membership.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :fee_paid
    = f.check_box :fee_paid
  .field
    = f.label :tshirt
    = f.check_box :tshirt
  .actions
    = f.submit 'Save'
