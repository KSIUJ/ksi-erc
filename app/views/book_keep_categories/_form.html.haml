= form_for @book_keep_category do |f|
  - if @book_keep_category.errors.any?
    #error_explanation
      %h2= "#{pluralize(@book_keep_category.errors.count, "error")} prohibited this book_keep_category from being saved:"
      %ul
        - @book_keep_category.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .actions
    = f.submit 'Save'
